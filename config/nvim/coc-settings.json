{
  "yaml.schemas": {
    "kubernetes": "/*.yaml",
    "http://json.schemastore.org/ansible-stable-2.5": "/*.yml",
    "https://raw.githubusercontent.com/docker/cli/master/cli/compose/schema/data/config_schema_v3.8.json": "/docker-compose.*yml",
    "http://json.schemastore.org/gitlab-ci": "/.gitlab-ci.yml"
  },
  "yaml.validate": true,
  "yaml.format.enable": true,
  "yaml.completion": true,
  "cSpellExt.enableDictionaries": ["russian"],
  "cSpell.language": "en,ru",
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "snippets.ultisnips.pythonPrompt": false
}
